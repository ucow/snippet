		"prefix": "page",
		"body": [
			"<div class='page $1'>",
			"\t<div class='navbar'>",
			"\t\t<div class='navbar-bg'></div>",
			"\t\t<div class='navbar-inner'>",
			"\t\t\t<div class='left'>",
			"\t\t\t\t<a href='#' class='link back'>",
			"\t\t\t\t\t<i class='icon icon-back'></i>",
			"\t\t\t\t</a>",
			"\t\t\t</div>",
			"\t\t\t<div class='title'>$2</div>",
			"\t\t</div>",
			"\t</div>",
			"\t<div class='page-content'>",
